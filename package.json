  "bugs": {
    "url": "https://github.com/{{param `github.owner`}}/{{param `github.repo`}}/issues"
  },
  "homepage": "https://github.com/{{param `github.owner`}}/{{param `github.repo`}}#readme",
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
